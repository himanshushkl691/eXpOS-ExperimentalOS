int main()
{
	decl
		int ret;
		str mystr;
	enddecl
	begin
		ret = exposcall("Write",-2,"Filename");
		ret = exposcall("Read",-1,mystr);
		ret = exposcall("Delete",mystr);
		if(ret == 0) then
			ret = exposcall("Write",-2,"Deleted");
		endif;
		if(ret == -1) then
			ret = exposcall("Write",-2,"Denied");
		endif;
		if(ret == -2) then
			ret = exposcall("Write",-2,"File Open");
		endif;
		return 0;
	end
}
