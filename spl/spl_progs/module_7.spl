[i + 11] = "0100";

//stack
[i + 16] = 78;
[i + 17] = "0110";
[i + 18] = 79;
[i + 19] = "0110";

i = PROCESS_TABLE + 32;
//pid
[i + 1] = 2;
//ppid
[i + 2] = 1;
//status
[i + 4] = TERMINATED;
//user area page number
[i + 11] = 80;
//user stack pointer
