//kernel stack pointer
[i + 12] = 0;
//ptbr
[i + 14] = (PAGE_TABLE_BASE + 40);
//ptlr
[i + 15] = 10;
[78 * 512] = [67 * 512 + 1];

//initialize disk-map table for shell
i = 0;
while(i < 10) do
	[DISK_MAP_TABLE + 20 + i] = -1;
	i = i + 1;
endwhile;
[DISK_MAP_TABLE + 24] = 67;
[DISK_MAP_TABLE + 25] = 68;
